      <section eId="sec_2111">
        <num>2111</num>
        <heading>Refusing certificate of title</heading>
        <subsection eId="sec_2111__subsec_a">
          <num>(a)</num>
          <content>
            <p>The applicant is not the owner of the vehicle;\n</p>
          </content>
        </subsection>
        <subsection eId="sec_2111__subsec_b">
          <num>(b)</num>
          <content>
            <p>The application contains a false or fraudulent statement; or\n</p>
          </content>
        </subsection>
        <subsection eId="sec_2111__subsec_c">
          <num>(c)</num>
          <content>
            <p>The applicant fails to furnish required information or documents\nor any additional information the commissioner reasonably requires.\n</p>
          </content>
        </subsection>
      </section>
