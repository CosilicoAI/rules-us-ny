      <section eId="sec_55_05">
        <num>55.05</num>
        <heading>Classifications of felonies and misdemeanors</heading>
        <subsection eId="sec_55_05__subsec_a">
          <num>(a)</num>
          <content>
            <p>Class A felonies;\n</p>
          </content>
        </subsection>
        <subsection eId="sec_55_05__subsec_b">
