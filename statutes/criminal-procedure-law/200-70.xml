      <section eId="sec_200_70">
        <num>200.70</num>
        <heading>Indictment; amendment of</heading>
        <subsection eId="sec_200_70__subsec_a">
          <num>(a)</num>
          <content>
            <p>A failure thereof to charge or state an offense; or\n</p>
          </content>
        </subsection>
        <subsection eId="sec_200_70__subsec_b">
          <num>(b)</num>
          <content>
            <p>Legal insufficiency of the factual allegations; or\n</p>
          </content>
        </subsection>
        <subsection eId="sec_200_70__subsec_c">
          <num>(c)</num>
          <content>
            <p>A misjoinder of offenses; or\n</p>
          </content>
        </subsection>
        <subsection eId="sec_200_70__subsec_d">
          <num>(d)</num>
          <content>
            <p>A misjoinder of defendants.\n</p>
          </content>
        </subsection>
      </section>
