    <body>
      <section eId="sec_6_1_1">
        <num>6-1.1</num>
        <heading>Estates classified</heading>
        <subsection eId="sec_6_1_1__subsec_a">
          <num>(a)</num>
          <content>
            <p>Estates in property as to duration are classified as follows:\n</p>
          </content>
          <paragraph eId="sec_6_1_1__subsec_a__subsec_1">
            <num>(1)</num>
            <content>
              <p>Fee simple absolute.\n</p>
            </content>
          </paragraph>
          <paragraph eId="sec_6_1_1__subsec_a__subsec_2">
            <num>(2)</num>
            <content>
              <p>Fee on condition.\n</p>
            </content>
          </paragraph>
          <paragraph eId="sec_6_1_1__subsec_a__subsec_3">
            <num>(3)</num>
            <content>
              <p>Fee on limitation.\n</p>
            </content>
          </paragraph>
          <paragraph eId="sec_6_1_1__subsec_a__subsec_4">
            <num>(4)</num>
            <content>
              <p>Estates for life.\n</p>
            </content>
          </paragraph>
          <paragraph eId="sec_6_1_1__subsec_a__subsec_5">
            <num>(5)</num>
            <content>
              <p>Estates for years.\n</p>
            </content>
          </paragraph>
          <paragraph eId="sec_6_1_1__subsec_a__subsec_6">
            <num>(6)</num>
            <content>
              <p>Estates from period to period.\n</p>
            </content>
          </paragraph>
          <paragraph eId="sec_6_1_1__subsec_a__subsec_7">
            <num>(7)</num>
            <content>
              <p>Estates at will.\n</p>
            </content>
          </paragraph>
          <paragraph eId="sec_6_1_1__subsec_a__subsec_8">
            <num>(8)</num>
            <content>
              <p>Estates by sufferance.\n</p>
            </content>
          </paragraph>
        </subsection>
      </section>
